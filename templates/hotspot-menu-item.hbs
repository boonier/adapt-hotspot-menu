<button class="menu-item-hotspot{{#if _isVisited}} visited{{/if}}{{#if _isComplete}} complete{{/if}}{{#if _isLocked}} locked icon icon-padlock-locked{{/if}}" style="top:{{_hotspotMenuItem.position._top}}%; left:{{_hotspotMenuItem.position._left}}%" data-id="{{_id}}" aria-label="" role="button"></button>
<div class="menu-item-inner{{#if _isLocked}} disabled{{/if}}{{#if _offsetPopupFromHotspot}} is-offset{{/if}}" {{#if _offsetPopupFromHotspot}}style="top:{{_hotspotMenuItem.position._top}}%; left:{{_hotspotMenuItem.position._left}}%"{{/if}}>
	<a href="#" class="menu-item-done icon icon-cross"></a>
	<div class="menu-item-title">{{{displayTitle}}}</div>
	<div class="menu-item-body">
		<div class="menu-item-body-inner">{{{body}}}</div>
	</div>
	{{#if _assessment}}
	<div>
		<div>Assessment Info:</div>
		<div>Completion status: {{#if _assessment.isComplete}}Complete{{else}}Not Completed{{/if}} </div>
		{{#if _assessment.hasScore}}
			<div>Last Attempt Score: {{_assessment.scoreAsPercentage}}%</div>
		{{/if}}
		<div>Passed status: {{#if _assessment.isPassed}}Passed{{else}}Not Passed{{/if}} </div>
	</div>
	{{/if}}
	<div class="menu-item-button">
		{{#unless _isLocked}}
			<a href="#/id/{{_id}}" class="{{#if _isVisited}}visited{{/if}}">
				{{{linkText}}}
			</a>
			<button data-id="{{_id}}" aria=""></button>
		{{/unless}}
		<span class="menu-item-duration">Duration: {{{duration}}}</span>
	</div>
{{!-- 	<div class="menu-item-progress">
		<div class="menu-item-progress-indicator">
			<div class="menu-item-progress-indicator-bar" style="width:{{completedChildrenAsPercentage}}%"></div>
		</div>
	</div> --}}
</div>
